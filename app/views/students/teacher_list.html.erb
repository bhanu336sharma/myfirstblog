<div class="container">
    <h1 id = "div1"></h1>
    <h1  align="center">TEACHER RECORDS</h1>
    <table class="table table-hover  table-bordered table-condensed ">
        <tr>
            <th>NAME</th>
        </tr>
        <% @teacher.each do |obj| %>
        <%if obj.role == 'teacher'%>
        <tr>
            <td>
                <%=obj.email%>
            </td>
        </tr>
        <%end%>
        <%end%>
    </table>
</div>

